<h3>ジャンル編集</h3>
<%= form_with model: @genre, url:admin_genre_path(@genre.id) do |f| %>
         <div class="form-group">
        　<label for="genre_name">ジャンル名</label><br>
        　<%= f.text_field :name, autofocus: true, id:"inputName" %>
         </div>
         <div class="form-group">
         <%= f.submit "変更を保存", class:"btn btn-success" %>
         </div>
<% end %>